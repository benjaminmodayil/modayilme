{
  "name": "personal-site",
  "description": "modayil.me",
  "version": "1.0.0",
  "author": "Benjamin Modayil <modayilme@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build:css": "postcss src/layouts/index.css -o src/layouts/generated.css",
    "watch:css": "postcss src/layouts/index.css -o src/layouts/generated.css -w",
    "build-0ld": "npm run build:css && purgecss -c ./purgecss.config.js -o ./src/layouts && gatsby build",
    "develop-0ld": "npm run watch:css & gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "dependencies": {
    "gatsby": "^2.1.4",
    "gatsby-image": "^2.0.29",
    "gatsby-plugin-manifest": "^2.0.17",
    "gatsby-plugin-netlify-cms": "^3.0.12",
    "gatsby-plugin-offline": "^2.0.23",
    "gatsby-plugin-postcss": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-sharp": "^2.0.20",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.0.20",
    "gatsby-transformer-sharp": "^2.1.13",
    "netlify-cms": "^2.4.2",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "tailwindcss-accessibility": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "cssnano": "^3.10.0",
    "gh-pages": "^1.0.0",
    "postcss-cli": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-nested": "^3.0.0",
    "prettier": "^1.6.1",
    "purgecss": "^1.0.1",
    "stylelint": "^8.2.0",
    "stylelint-order": "^0.7.0",
    "tailwindcss": "^0.5.2"
  },
  "browserslist": [
    "> 1%",
    "IE >= 10",
    "last 2 versions"
  ]
}
